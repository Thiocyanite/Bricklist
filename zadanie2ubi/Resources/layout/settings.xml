<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:minWidth="25px"
	android:minHeight="25px"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/linearLayout1" >
	<LinearLayout
		android:orientation="horizontal"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="69.5dp"
		android:id="@+id/linearLayout2">
		<TextView
			android:text="Nazwa konta Gmail:"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="128.0dp"
			android:layout_height="match_parent"
			android:id="@+id/textView1"
			android:layout_marginBottom="0.0dp" />
		<EditText
			android:layout_width="204.5dp"
			android:layout_height="match_parent"
			android:id="@+id/editText2" />
	</LinearLayout>
	<LinearLayout
		android:orientation="horizontal"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="74.5dp"
		android:id="@+id/linearLayout3">
		<TextView
			android:text="Hasło"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="112.5dp"
			android:layout_height="44.5dp"
			android:id="@+id/textView2" />
		<EditText
			android:inputType="textPassword"
			android:layout_width="220.0dp"
			android:layout_height="54.0dp"
			android:id="@+id/editText1" />
	</LinearLayout>
	<Button
		android:text="Eksportuj"
		android:layout_width="384.0dp"
		android:layout_height="wrap_content"
		android:id="@+id/button1" />
	<Button
		android:text="Do menu"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/button2" />
</LinearLayout>