<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:minWidth="25px"
	android:minHeight="25px"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/linearLayout1">
	<LinearLayout
		android:orientation="horizontal"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="69.5dp"
		android:id="@+id/linearLayout2">
		<TextView
			android:text="Ścieżka zapisu"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="128.0dp"
			android:layout_height="match_parent"
			android:id="@+id/textView1"
			android:layout_marginBottom="0.0dp" />
		<EditText
			android:layout_width="204.5dp"
			android:layout_height="match_parent"
			android:id="@+id/editText2" />
	</LinearLayout>
	<Button
		android:text="Eksportuj"
		android:layout_width="384.0dp"
		android:layout_height="wrap_content"
		android:id="@+id/button1" />
	<Button
		android:text="Do menu"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/button2" />
	<TextView
		android:text="Large Text"
		android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="match_parent"
		android:layout_height="165.5dp"
		android:id="@+id/textView2" />
</LinearLayout>